{
  "name": "theknittygritty",
  "version": "2.0.0",
  "description": "Full-stack eCommerce application that utilizes React, Vite, Redux, Express, Node, and MongoDB.",
  "type": "module",
  "main": "server.js",
  "dependencies": {
    "acorn": "^8.14.1",
    "acorn-walk": "^8.3.4",
    "arg": "^4.1.3",
    "bcryptjs": "^3.0.2",
    "colors": "^1.4.0",
    "cookie-parser": "^1.4.7",
    "create-require": "^1.1.1",
    "diff": "^4.0.2",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "make-error": "^1.3.6",
    "mongoose": "^8.13.2",
    "multer": "^1.4.5-lts.2",
    "react-toastify": "^11.0.5",
    "undici-types": "^6.21.0",
    "v8-compile-cache-lib": "^3.0.1",
    "yn": "^3.1.1"
  },
  "scripts": {
    "build": "npm install && npm install --prefix frontend && tsc --project backend/tsconfig.json && npm run build --prefix frontend",
    "start": "node backend/dist/backend/server.js",
    "server": "nodemon",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node --loader ts-node/esm backend/seeder.ts",
    "data:destroy": "node --loader ts-node/esm backend/seeder.ts -d"
  },
  "author": "Romie Hecdivert",
  "license": "MIT",
  "devDependencies": {
    "@types/bcryptjs": "^3.0.0",
    "@types/cookie-parser": "^1.4.8",
    "@types/express": "^5.0.1",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.14.1",
    "concurrently": "^9.1.2",
    "dotenv": "^16.5.0",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
